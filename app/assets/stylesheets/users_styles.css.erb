.profile-wrapper {
  width: 93%;
  height: 500px;
  padding: 5px;
  display: block;
  margin: 0 auto;
  margin-top: 70px;
  background-color: #DDDDDD;
  color: #25282E;
}

.profile-wrapper a {
  font-weight: bolder;
}

.profile-wrapper ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.user-info {
  float: left;
  width: 40%;
}

.user-history {
  float: right;
  width: 50%;
}

.user-info ul {
  font-size: .75em;
}
